    "def madesystem(x):\n",
    "    system=[]\n",
    "    \n",
    "    for i in range(100):\n",
    "        a=numpy.random.random()\n",
    "        if a>=x/100:\n",
    "            system.append(1)\n",
    "        else:\n",
    "            system.append(0)\n",
    "    return system\n",
    "\n",
    "system_set=[]\n",
    "eigen_set=[]\n",
    "\n",
    "for i in range(0,99,1) :\n",
    "    system_set.append(madesystem(i))\n",
    "    eigen_set.append(ising_eigenvalue(system_set[i]))\n",
    "    \n",
    "plt.plot(range(0,99,1),eigen_set)\n",
