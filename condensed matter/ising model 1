    "import numpy\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "spin=[0,1]\n",
    "system1=[]\n",
    "for i in range(1,100):\n",
    "    system1.append(spin[numpy.random.randint(2)])\n",
    "\n",
    "\n",
    "def ising_eigenvalue(A):\n",
    "    s=0\n",
    "    for i in range(len(A)):\n",
    "        if A[i]==A[i-1]:\n",
    "            s += 1\n",
    "    return s\n",
    "\n",
    "print(system1)\n",
    "print(ising_eigenvalue(system1))"
   ]
